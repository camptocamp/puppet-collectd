{
  "name": "camptocamp-collectd",
  "version": "1.0.0",
  "author": "camptocamp",
  "license": "Apache-2.0",
  "source": "https://github.com/camptocamp/puppet-collectd.git",
  "project_page": "https://github.com/camptocamp/puppet-collectd",
  "issues_url": "https://github.com/camptocamp/puppet-collectd/issues",
  "summary": "Install and configure collectd on Debian and RedHat systems",
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">=2.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.2.0 <7.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04",
        "18.04"
      ]
    }
  ]
}
